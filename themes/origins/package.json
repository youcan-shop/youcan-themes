{
  "name": "origin",
  "type": "module",
  "scripts": {
    "sass": "sass ./styles:./.temp --no-source-map --watch",
    "postcss": "postcss ./.temp/*.css --dir ./assets",
    "watch:postcss": "onchange './.temp/*.css' -- pnpm run postcss",
    "yc": "youcan theme:dev -p",
    "dev": "npm-run-all --parallel sass watch:postcss yc"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@shopify/prettier-plugin-liquid": "^0.4.3",
    "child_process": "^1.0.2",
    "clean-css-cli": "^5.6.1",
    "concurrently": "^7.6.0",
    "eslint": "^9.16.0",
    "fs": "0.0.1-security",
    "globals": "^15.13.0",
    "node-watch": "^0.7.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "1.12.1",
    "sass": "^1.56.1",
    "strip-css-comments-cli": "^3.0.0"
  }
}