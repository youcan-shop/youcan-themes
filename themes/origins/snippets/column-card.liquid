{{ 'column-card.css' | asset_url | stylesheet_tag }}

<div class="column-card" data-position="center">
  <div class="column-img">
    <img
      width="auto"
      height="100px"
      data-ratio="small"
      data-shape="circle"
      alt="column title"
      src="{{ 'default-test.webp' |  asset_url }}"
    >
  </div>
  <div class="column-info">
    <h4 class="column-title">Planet friendly</h4>
    <h4 class="column-subtitle">Sustainable fabrics made with care.</h4>
  </div>
</div>
